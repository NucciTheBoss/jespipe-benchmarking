<?xml version="1.0" encoding="UTF-8"?>
<simu>
    <train>
        <dataset file="energy-appliances-weather/energy-appliances-weather.csv">
            <model plugin="plugins/models/RNN/LSTM.py">
                <name value="energy-appliances-weather-one-attack-one-plot" />

                <algorithm value="LSTM" />

                <parameters>
                    <sequence_length type="int" value="24" />
                    <batch_size type="int" value="128" />
                    <epochs type="int" value="500" />
                    <validation_split type="float" value="0.1" />
                    <verbose type="bool" value="True" />
                    <learning_rate type="float" value="0.001" />
                </parameters>

                <xgboost plugin="plugins/manips/many_to_one_xgb.py" tag="xgb1-energy">
                    <n_features type="int" value="9" />
                </xgboost>

                <randomforest plugin="plugins/manips/many_to_one_randomforest.py" tag="rf1-energy"></randomforest>

                <pca plugin="plugins/manips/many_to_one_pca.py" tag="pca1-energy">
                    <n_features type="int" value="9" />
                </pca>

                <candlestick plugin="plugins/manips/many_to_one_candlestick.py" tag="cand1-energy">
                    <time_interval type="int" value="20" />
                </candlestick>

                <vanilla plugin="plugins/manips/many_to_one_vanilla.py" tag="vanilla1-energy"></vanilla>
            </model>
        </dataset>

        <dataset file="parkinsons-telemonitoring/parkinsons-telemonitoring.csv">
            <model plugin="plugins/models/RNN/LSTM.py">
                <name value="parkinsons-telemonitoring-one-attack-one-plot" />

                <algorithm value="LSTM" />

                <parameters>
                    <sequence_length type="int" value="6" />
                    <batch_size type="int" value="64" />
                    <epochs type="int" value="200" />
                    <validation_split type="float" value="0.1" />
                    <verbose type="bool" value="True" />
                    <learning_rate type="float" value="0.001" />
                </parameters>

                <xgboost plugin="plugins/manips/many_to_one_xgb.py" tag="xgb1-parkinsons">
                    <n_features type="int" value="5" />
                </xgboost>

                <randomforest plugin="plugins/manips/many_to_one_randomforest.py" tag="rf1-parkinsons"></randomforest>

                <pca plugin="plugins/manips/many_to_one_pca.py" tag="pca1-parkinsons">
                    <n_features type="int" value="5" />
                </pca>

                <candlestick plugin="plugins/manips/many_to_one_candlestick.py" tag="cand1-parkinsons">
                    <time_interval type="int" value="20" />
                </candlestick>

                <vanilla plugin="plugins/manips/many_to_one_vanilla.py" tag="vanilla1-parkinsons"></vanilla>
            </model>
        </dataset>
    </train>
    <attack>
        <dataset file="energy-appliances-weather/energy-appliances-weather.csv">
            <CW_L2 plugin="plugins/attacks/carlinil2.py" model_plugin="plugins/models/RNN/LSTM.py" tag="cw_l2_1">
                <max_change type="float" value="0.2" />
                <min_change type="float" value="0.05" />
                <change_step type="float" value="0.025" />
                <learning_rate type="float" value="0.001" />
                <max_iter type="int" value="100" />
                <binary_search_steps type="int" value="9" />
                <batch_size type="int" value="20" />
                <initial_cost type="int" value="1" />
                <sequence_length type="int" value="60" />
                <verbose type="bool" value="True" />
            </CW_L2>
            <CW_Linf plugin="examples/plugins/attacks/carlinilinf.py" model_plugin="examples/plugins/models/RNN/LSTM.py" tag="cw_linf_1">
                <max_change type="float" value="0.2" />
                <min_change type="float" value="0.05" />
                <change_step type="float" value="0.025" />
                <learning_rate type="float" value="0.001" />
                <max_iter type="int" value="300" />
                <batch_size type="int" value="5" />
                <initial_cost type="float" value="0.1" />
                <largest_const type="int" value="100" />
                <sequence_length type="int" value="36" />
                <decrease_factor type="float" value="0.9" />
                <verbose type="bool" value="True" />
            </CW_Linf>
        </dataset>

        <dataset file="parkinsons-telemonitoring/parkinsons-telemonitoring.csv">
            <CW_L2 plugin="plugins/attacks/carlinil2.py" model_plugin="plugins/models/RNN/LSTM.py" tag="cw_l2_1">
                <max_change type="float" value="0.2" />
                <min_change type="float" value="0.05" />
                <change_step type="float" value="0.025" />
                <learning_rate type="float" value="0.001" />
                <max_iter type="int" value="100" />
                <binary_search_steps type="int" value="9" />
                <batch_size type="int" value="20" />
                <initial_cost type="int" value="1" />
                <sequence_length type="int" value="60" />
                <verbose type="bool" value="True" />
            </CW_L2>
            <CW_Linf plugin="examples/plugins/attacks/carlinilinf.py" model_plugin="examples/plugins/models/RNN/LSTM.py" tag="cw_linf_1">
                <max_change type="float" value="0.2" />
                <min_change type="float" value="0.05" />
                <change_step type="float" value="0.025" />
                <learning_rate type="float" value="0.001" />
                <max_iter type="int" value="300" />
                <batch_size type="int" value="5" />
                <initial_cost type="float" value="0.1" />
                <largest_const type="int" value="100" />
                <sequence_length type="int" value="36" />
                <decrease_factor type="float" value="0.9" />
                <verbose type="bool" value="True" />
            </CW_Linf>
        </dataset>
    </attack>
    <clean>
        <clear_tmp />
        <plot plugin="plugins/plots/plot_rmse_si_mae.py" tag="energy-weather-appliances">
            <tag value="xgb1-energy" />
            <tag value="rf1-energy" />
            <tag value="pca1-energy" />
            <tag value="cand1-energy" />
            <tag value="vanilla1-energy" />
        </plot>
        <plot plugin="plugins/plots/plot_rmse_si_mae.py" tag="energy-weather-appliances">
            <tag value="xgb1-parkinsons" />
            <tag value="rf1-parkinsons" />
            <tag value="pca1-parkinsons" />
            <tag value="cand1-parkinsons" />
            <tag value="vanilla1-parkinsons" />
        </plot>
        <compress>
            <format value="gzip" />
            <name value="ten-model-two-attack-two-plot" />
            <path value="." />
        </compress>
    </clean>
</simu>
